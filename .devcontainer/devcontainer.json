{
    "name": "golang dev",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "vscode",
    "runServices": [
        "vscode"
    ],
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/workspace",
    "extensions": [
        "golang.go",
        "git.gitgraph"
    ],
    "settings": {
        "files.eol": "\n",
        "remote.extensionKind": {
            "ms-azuretools.vscode-docker": "workspace"
        },
        "go.useLanguageServer": true,
    }
}